{
  "follow": true,
  "keymap": {
    "bigger": ["+", "="],
    "change_focus": ["c"],
    "change_focus_up": ["C"],
    "clear_filter": ["F", "E"],
    "clear_selection": ["S"],
    "collapse": ["<s-tab>", "`"],
    "copy": ["p"],
    "copy_name": ["y"],
    "cut": ["x"],
    "delete": ["d"],
    "filter": ["f"],
    "h_split": ["W"],
    "jump_to_current": ["J"],
    "new": ["a"],
    "open_sys": ["O"],
    "primary": ["o", "<enter>"],
    "quit": ["q"],
    "refocus": ["~"],
    "refresh": ["<c-r>"],
    "rename": ["r"],
    "search": ["e"],
    "secondary": ["<tab>", "<2-leftmouse>"],
    "select": ["s"],
    "smaller": ["-", "_"],
    "stat": ["K"],
    "tertiary": ["t", "<middlemouse>"],
    "toggle_follow": [],
    "toggle_hidden": ["."],
    "toggle_version_control": [],
    "v_split": ["w"]
  },
  "logging_level": "WARNING",
  "mimetypes": {
    "ignore_exts": [".ts"],
    "warn": ["audio", "font", "image", "video"]
  },
  "open_left": true,
  "session": true,
  "show_hidden": false,
  "update_time": {
    "max": 2,
    "min": 1
  },
  "use_icons": true,
  "version_control": {
    "defer": false,
    "enable": true
  },
  "width": 40
}
